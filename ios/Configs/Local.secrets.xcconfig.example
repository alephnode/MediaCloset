// Hasura GraphQL endpoint for data persistence
GRAPHQL_ENDPOINT = https://your-hasura-instance.hasura.app/v1/graphql
HASURA_ADMIN_SECRET = your-admin-secret-here

// MediaCloset Go API endpoint and key
MEDIACLOSET_API_ENDPOINT = http://localhost:8080/query
MEDIACLOSET_API_KEY = your-secure-api-key-here

// External API keys
DISCOGS_CONSUMER_KEY = bar
DISCOGS_CONSUMER_SECRET = bizz
LASTFM_API_KEY = fizz
